{
  "name": "zak-fisher",
  "version": "1.0.0",
  "description": "Zak Fisher's Website",
  "main": "index.js",
  "private": true,
  "scripts": {
    "deploy": "NODE_ENV=production npm run build && ...",
    "start": "npm run start:dev",
    "start:dev": "NODE_ENV=development webpack-dev-server --config webpack.lint.config.js --content-base app/public --inline --history-api-fallback",

    "start:prod:spa": "NODE_ENV=production npm run build:spa && serve build",
    "build:spa": "npm run build:client:spa && npm run build:docs",
    "build:client:spa": "webpack --config webpack.prod.spa.config.js",

    "start:prod:express": "NODE_ENV=production npm run build:express && node server.bundle.js",
    "build:express": "npm run build:client:express && npm run build:server && npm run build:docs",
    "build:client:express": "webpack --config webpack.prod.express.config.js",
    "build:server": "webpack --config webpack.server.config.js",

    "build:docs": "documentation build app/index.js -f html -o build/docs",
    "lint": "npm run slint && npm run elint",
    "elint": "eslint . --ext .js --ext .jsx --cache || true",
    "elinttable": "eslint . --ext .js --ext .jsx --cache --format table || true",
    "elintsummary": "eslint . --ext .js --ext .jsx --cache --format 'node_modules/eslint-friendly-formatter' || true",
    "slint": "stylelint ./app/styles/**/*.css ./app/style.css --syntax scss || true",
    "test": "NODE_ENV=test mocha test/browser.js test/**/*.spec.js --compilers js:babel-core/register --recursive || true",
    "cover": "NODE_ENV=test babel-node node_modules/.bin/babel-istanbul cover _mocha --  --require test/browser.js test/**/*.spec.js --reporter dot || true",
    "profile": "NODE_ENV=production webpack --config webpack.prod.express.config.js --profile --json > profile.json",
    "storybook": "start-storybook -p 9001 -s ./app/public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zakfisher/zak-fisher.com.git"
  },
  "author": "Zak Fisher",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zakfisher/zak-fisher.com/issues"
  },
  "homepage": "https://github.com/zakfisher/zak-fisher.com#readme",
  "dependencies": {
    "firebase": "^3.3.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "@kadira/storybook": "^2.5.2",
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-istanbul": "^0.11.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-airbnb": "^2.0.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-es2017": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.13.0",
    "browser-sync": "^2.14.0",
    "browser-sync-webpack-plugin": "^1.1.2",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.10",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "documentation": "^4.0.0-beta13",
    "enzyme": "^2.4.1",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-loader": "^0.4.4",
    "html-minifier": "^3.0.2",
    "html-webpack-plugin": "^2.22.0",
    "jsdom": "^9.4.2",
    "json-loader": "^0.5.4",
    "mocha": "^3.0.2",
    "normalize.css": "^4.2.0",
    "postcss-easy-import": "^1.0.1",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^15.3.1",
    "sinon": "^1.17.5",
    "style-loader": "^0.13.1",
    "stylelint": "^7.1.0",
    "stylelint-config-standard": "^12.0.0",
    "stylelint-webpack-plugin": "^0.3.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "engines": {
    "node": ">=5.0 <7",
    "npm": ">=3.3 <4"
  }
}
